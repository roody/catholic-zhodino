<!-- Top border -->
<div class="hdr-brd-tc">
    <div class="hdr-brd-tl"></div>
    <div class="hdr-brd-tr"></div>
</div>

<!-- Top spacer -->
<div class="hdr-spcr-tc">
    <div class="hdr-spcr-tl"></div>
    <div class="hdr-spcr-tr"></div>
</div>

<!-- Header content -->
<div class="hdr-content">
    <div class="hdr-logo">
        <div class="hdr-logo-brd-l"></div>
        <div class="hdr-logo-content">
            <img class="logo-image" src="/images/header/logo.png" alt="<%= t("header.url") %>"
                 title="<%= t("header.url") %>">
        </div>
    </div>
    <div class="hdr-logo-strip"></div>
    <div class="main-title-cntr">
        <h1 class="main-title"><%= t("header.official_page") %></h1>
        <h1 class="main-title" style="margin-left: 100px;"><%= t("header.parish") %></h1>
        <h2 class="sub-title"><%= t("header.zhodino") %></h2>
    </div>
    <div class="hdr-content-cloud"></div>
</div>

<!-- Header bottom -->
<div class="hdr-bottom">
    <div class="hdr-logo-bottom"></div>
    <div class="hdr-bottom-r"></div>
</div>